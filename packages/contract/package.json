{
  "name": "@repo/launchpad-contract",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "compile": "compact compile --skip-zk src/main.compact ./src/managed/statera-launchpad",
    "build": "rm -rf dist && tsc --project tsconfig.build.json && cp -R ./src/managed ./dist/managed",
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "typescript": "^5.8.2",
    "vitest": "^4.0.1"
  }
}
